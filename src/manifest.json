{
  "name": "CRead",
  "description": "continue reading web",
  "version": "0.0.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "permissions": [
    "storage",
    "tabs",
    "scripting"
  ],
  "host_permissions": [
    "https://*/*",
    "http://*/*"
  ],
  "action": {
    "default_popup": "popup/index.html"
  },
  "icons": {
    "16": "CR16.png",
    "48": "CR48.png",
    "128": "CR128.png"
  }
}